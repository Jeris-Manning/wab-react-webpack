    "eslint-plugin-react": "^7.4.0",
    "gulp": "^3.9.1",
    "gulp-babel": "^6.1.2",
    "jest": "^21.2.1",
    "lodash-webpack-plugin": "^0.11.4",
    "prettier": "^1.7.0",
    "react": "^16.4.1",
    "react-dom": "^16.4.1",
    "react-test-renderer": "^16.0.0",
    "replace-in-file": "^3.1.1",
    "style-loader": "^0.19.1",
    "webpack": "^3.8.1",
    "webpack-stream": "^4.0.0"
